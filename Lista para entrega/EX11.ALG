Algoritmo "semnome"

Var
n1,n2,n3: inteiro


Inicio
escreval ("Insira um número")
leia (n1)

escreval ("Insira um número")
leia (n2)

escreval ("Insira um número")
leia (n3)

se (n1>n2) e (n1>n3) entao
 escreval (n1," é o maior")
senao
 se n2>n3 entao
  escreval (n2," é o maior")
 senao
   escreval (n3," é o maior")
 fimse
fimse


Fimalgoritmo